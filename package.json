{
  "name": "assemblyscript-regex",
  "version": "0.1.0",
  "description": "A regex engine built with AssemblyScript",
  "ascMain": "assembly/index.ts",
  "scripts": {
    "test": "npm run asbuild:untouched && npm run test:generate && npm run prettier:check && jest __tests__ --reporter=jest-summary-reporter",
    "test:generate": "node __spec_tests__/test-generator.js",
    "jest": "jest __tests__ --testPathIgnorePatterns assembly/*",
    "asp": "asp --verbose",
    "asp:ci": "asp",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "asbuild:untouched": "asb --target debug",
    "asbuild:optimized": "asb",
    "asbuild": "npm run asbuild:untouched && npm run asbuild:optimized",
    "tsrun": "ts-node ts/index.ts",
    "benchmark": "node benchmark/benchmark.js"
  },
  "author": "colin.eberhardt@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@as-pect/cli": "^5.0.1",
    "@assemblyscript/loader": "^0.17.5",
    "@types/node": "^14.14.13",
    "asbuild": "0.0.10",
    "assemblyscript": "0.17.5",
    "benchmark": "^2.1.4",
    "jest": "^26.6.3",
    "jest-summary-reporter": "0.0.2",
    "prettier": "^2.2.1",
    "text-encoding": "^0.7.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "visitor-as": "^0.3.2"
  },
  "dependencies": {}
}
